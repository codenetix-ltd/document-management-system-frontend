{
  "name": "dms-frontend",
  "version": "0.0.1",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --debug --watch --config webpack.dev.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --debug --watch --open --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.prod.js",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:update": "cross-env NODE_ENV=test jest --verbose --updateSnapshot",
    "test:watch": "cross-env NODE_ENV=test jest --verbose --watch",
    "test:watch-all": "cross-env NODE_ENV=test jest --verbose --watchAll",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --collectCoverageFrom=src/**/*.{js,jsx}",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "dependencies": {
    "array-merge-by-key": "^1.0.1",
    "autobind-decorator": "^2.1.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "prop-types": "^15.6.1",
    "qc-react-conditionals": "^0.0.0-alpha",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-datetime": "^2.14.0",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.3.2",
    "react-loadable": "^5.4.0",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.0.7",
    "react-responsive-modal": "^3.0.2",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-sortable-hoc": "^0.7.2",
    "react-table": "^6.8.5",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "secure-ls": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "connect-api-mocker": "^1.3.6",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
